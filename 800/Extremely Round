import java.util.*;
 
public class Solution {
    public static void main(String[] args) {
    	Scanner sc = new Scanner(System.in);
        int t = sc.nextInt();
        while (t-- > 0) {
        	int n = sc.nextInt();
        	int res=  0;	
        	String str = String.valueOf(n);
        	for(int i=0; i<str.length()-1; i++){
        		res+=9;
        	}
        	res+=(int)(str.charAt(0)-'0');
        	System.out.println(res);
 
        }
    }
}